@model DiscoveryCenter.Models.MultipleSelectViewModel
@for (int z = 0; z < @Model.Choices.Count; z++)
{
    @Html.Hidden(String.Format("{0}_Options[{1}].text", Model.QuestionId, z),Model.Options[z].text)
    @Html.CheckBox(String.Format("{0}_Options[{1}].IsSelected",Model.QuestionId,z),Model.Options[z].IsSelected)
    @Html.Label(Model.Choices[z])
    <br />
}
